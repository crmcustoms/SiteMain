  "scripts": {
    "dev": "next dev",
    "dev:watch": "nodemon --watch app --watch components --watch lib --ext js,jsx,ts,tsx,json --exec \"next dev\"",
    "build": "next build",
    "start": "next start",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "clean:build": "rimraf .next && npm run build",
    "prod": "npm run clean:build && npm run start",
    "lint": "next lint",
    "copy-public-linux": "if [ -d \".next/standalone\" ]; then cp -r public .next/standalone/; fi",
    "copy-express-linux": "if [ -d \".next/standalone\" ]; then cp express-server.js .next/standalone/; fi",
